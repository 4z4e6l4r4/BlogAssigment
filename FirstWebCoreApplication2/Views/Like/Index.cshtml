@model IEnumerable<FirstWebCoreApplication2.Models.BlogLike>

@{
    ViewData["Title"] = "All Likes";

}
<div class="container">
    <div class="row">

        <div class="col-12">
            <div class="card p-3">
                <h3>Like List</h3>
                <hr />
                <table class="table table-hover table-bordered table-striped">
                    <thead>
                        <tr>
                            <th class="text-center">Name</th>
                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var like in Model)
                        {
                            <tr>
                                <td class="bg-primary">
                                        <i class="fa fa-user"></i> @like.User.Username
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>

            </div>
        </div>
    </div>
</div>
